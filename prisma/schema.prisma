
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model UserModel {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username  String
  password  String
  refreshToken String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}