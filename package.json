{
	"name": "express-api",
	"version": "1.0.0",
	"description": "Sandbox project for express api",
	"main": "index.js",
	"scripts": {
		"build": "npx prisma generate && tsc",
		"start": "npx prisma migrate deploy && node ./dist/src/main.js",
		"dev": "tsx watch --include ./src --exclude ./src/**/*.spec.ts --clear-screen=false ./src/main.ts",
		"dev:inspect": "tsx --inspect-brk ./src/main.ts",
		"db:up": "docker compose up -d",
		"db:down": "docker compose down",
		"db:restart": "docker compose down && docker compose up -d",
		"lint": "eslint ./src/**",
		"lint:fix": "eslint ./src/** --fix",
		"test": "jest"
	},
	"author": "Vadim Maul",
	"license": "ISC",
	"dependencies": {
		"@prisma/adapter-pg": "^7.0.1",
		"@prisma/client": "^7.0.1",
		"bcryptjs": "^3.0.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.3",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"inversify": "^7.10.4",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.16.3",
		"reflect-metadata": "^0.2.2",
		"tslog": "^4.10.2"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.10.1",
		"@types/pg": "^8.15.6",
		"@typescript-eslint/eslint-plugin": "^8.48.0",
		"@typescript-eslint/parser": "^8.48.0",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"jest": "^30.2.0",
		"prettier": "^3.7.1",
		"prisma": "^7.0.1",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0"
	}
}
