{
	"name": "express-api",
	"version": "1.0.0",
	"description": "Sandbox project for express api",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"start": "node ./dist/main.js",
		"build": "tsc",
		"dev": "tsx watch --include ./src --exclude ./src/**/*.spec.ts --clear-screen=false ./src/main.ts",
		"dev:inspect": "tsx --inspect-brk ./src/main.ts",
		"lint": "eslint ./src/**",
		"lint:fix": "eslint ./src/** --fix",
		"test": "echo \"Error: no test specified\" && exit 0"
	},
	"author": "Vadim Maul",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^3.0.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.3",
		"express": "^5.1.0",
		"inversify": "^7.10.4",
		"reflect-metadata": "^0.2.2",
		"tslog": "^4.10.2"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/express": "^5.0.5",
		"@typescript-eslint/eslint-plugin": "^8.48.0",
		"@typescript-eslint/parser": "^8.48.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"prettier": "^3.7.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0"
	}
}
